{{!
    Page header.
}}
<header role="banner" class="navbar navbar-full navbar-inverted navbar-fixed-top moodle-has-zindex">
    <div class="container-fluid">
        <a role="banner" href="{{{ config.wwwroot }}}" class="navbar-brand">
            <div class="logo">
                {{#output.get_logo_url }}
                    <img src="{{ output.get_logo_url }}" alt="{{sitename}}" class="hidden-xs-down">
                {{/output.get_logo_url}}

                {{#output.get_logocompact_url }}
                    <img src="{{ output.get_logocompact_url }}" alt="{{sitename}}" class="hidden-sm-up">
                {{/output.get_logocompact_url}}
            </div>
        </a>
        


        <div class="navigation">
            <!-- search_box -->
            {{{ output.search_box }}}

            <!-- navbar_plugin_output -->
            {{{ output.navbar_plugin_output }}}

            <!-- user_menu -->
            {{{ output.user_menu }}}
        </div>

        <button class="navbar-toggler" type="button">â˜°<span class="sr-only">{{#str}}expand{{/str}}</span></button>

        {{# output.show_cog }}
           {{{ output.show_cog }}}
        {{/ output.show_cog }}

        <div class="collapse navbar-toggleable-md" id="bd-main-nav">
            <nav class="nav navbar-nav">
                <!-- custom_menu -->
                {{{ output.custom_menu }}}
                <!-- page_heading_menu -->
                {{{ output.page_heading_menu }}}
            </nav>
        </div>
    </div>
</header>

