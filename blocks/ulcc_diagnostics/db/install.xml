<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="blocks/ulcc_diagnostics/db" VERSION="20070815" COMMENT="XMLDB file for ULCC Diagnostic Block" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd" 
>
  <TABLES>
    <TABLE NAME="ulcc_admin_flags" COMMENT="Flags external IDs for bulk upload">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" ENUM="false" NEXT="itemid"/>
        <FIELD NAME="itemid" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="false" ENUM="false" PREVIOUS="id" NEXT="tablename"/>
        <FIELD NAME="tablename" TYPE="char" LENGTH="255" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" PREVIOUS="itemid" NEXT="externalid"/>
        <FIELD NAME="externalid" TYPE="char" LENGTH="255" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" PREVIOUS="tablename"/>
       </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id" COMMENT="Primary key for block_ilp"/>
      </KEYS>
    </TABLE>
  </TABLES>
</XMLDB>